<%= render 'layouts/header' %>

<h1>create new dish</h1>

<%= form_for @dish do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :course %>
  <%= f.text_field :course %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.submit 'submit' %>
<% end %>
